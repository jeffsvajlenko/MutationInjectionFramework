    {
        switch (s->checksum->type)
        {
        case HASH_MD5:
            printf(" %-20s = if failed MD5(%s) then %s\n",
                   "Checksum", s->checksum->hash,
                   actionnames[s->checksum->action]);
            break;
        case HASH_SHA1:
            printf(" %-20s = if failed SHA1(%s) then %s\n",
                   "Checksum", s->checksum->hash,
                   actionnames[s->checksum->action]);
            break;
        default:
            printf(" %-20s = if failed UNKNOWN(%s) then %s\n",
                   "Checksum", s->checksum->hash,
                   actionnames[s->checksum->action]);
            break;
        }
    }